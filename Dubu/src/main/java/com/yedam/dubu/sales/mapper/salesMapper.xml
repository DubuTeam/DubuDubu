<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.dubu.sales.mapper.SalesMapper">


	<select id="searchSalesList" resultType="SalesVO">
		SELECT
		ORDER_NO, EDCTS_CD, VEND_CD, ORDER_DT, VEND_NM, PRDT_NM, ORDER_CNT,
		PROG_APPE, PAPRD_DT
		FROM ORDER_SHEET
		WHERE VEND_NM LIKE '%${vendNm}%'

	</select>
	<delete id="deleteSalesInfo" parameterType="int">
		DELETE FROM ORDER_SHEET
		WHERE ORDER_NO = #{orderNo}

	</delete>
</mapper>